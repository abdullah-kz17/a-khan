generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Booking {
  id          Int      @id @default(autoincrement())
  patientName String
  phone       String
  date        String
  time        String
  service     String
  status      String   @default("PENDING")
  createdAt   DateTime @default(now())
}
